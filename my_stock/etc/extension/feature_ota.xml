    <app_feature name="com.oplus.ota.component_update_url" args="String:https://component-ota-eu.allawnos.com"/>
    <!--show_appointment_entry ,default:true-->
    <app_feature name="com.oplus.ota.show_appointment_entry" args="boolean:true"/>
    <!--cellular_download_choice_type ,default:1-->
    <app_feature name="com.oplus.ota.cellular_download_choice_type" args="int:1"/>
