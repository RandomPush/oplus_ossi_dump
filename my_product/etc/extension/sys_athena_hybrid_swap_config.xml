<?xml version="1.0" encoding="utf-8"?>
<filter-conf>
    <version>2022060601</version>
    <filter-name>sys_athena_hybrid_swap_config</filter-name>
    <hybrid_swap_config>
        <scene name="boot">
            <ram value="all">
                <hybrid_swap_action_config action="0" cgroup="#" label="100" value="1" />
                <hybrid_swap_action_config action="1" cgroup="#" label="101" value="1" />
