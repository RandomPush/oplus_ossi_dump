        <DefaultWallpaperLock>NA</DefaultWallpaperLock>
        <DefaultWallpaperComponent>com.android.wallpaper.livepicker/.service.video.op.Salami0VideoWallpaper</DefaultWallpaperComponent>
    </PhoneColor>

