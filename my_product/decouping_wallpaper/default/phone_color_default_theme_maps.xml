<?xml version="1.0" encoding="UTF-8"?>
<PhoneColors>
        <!--PhoneColor.name：手机颜色的唯一标识，由项目、手软提供-->
        <!--ColorDescription：手机颜色描述，描述手机颜色-->
        <!--ColorDescriptionCN：手机颜色描述（中文），描述手机颜色-->
        <!--DefaultWallpaper :对应的锁屏壁纸-->
        <!--DefaultWallpaperLock :对应的桌面壁纸-->
        <!--DefaultWallpaperComponent :对应的动态壁纸-->
        <!--ThemeDescriptionCN：主题描述（中文），描述主题名称-->
        <!--手机颜色唯一标识、手机颜色描述由项目或手软提供，对应主题由桌面主题组根据UI要求配置-->
        <!--该文件由OS产品中心桌面主题组维护-->

    <PhoneColor name="FF99CCCC">
        <ColorDescription>Misty Green</ColorDescription>
        <ColorDescriptionCN>青釉</ColorDescriptionCN>
        <DefaultWallpaper>default_wallpaper_green</DefaultWallpaper>
        <DefaultWallpaperLock>default_wallpaper_green</DefaultWallpaperLock>
        <DefaultWallpaperComponent>NA</DefaultWallpaperComponent>
    </PhoneColor>

    <PhoneColor name="FF666666">
        <ColorDescription>Tempest Gray</ColorDescription>
        <ColorDescriptionCN>黑岩</ColorDescriptionCN>
        <DefaultWallpaper>default_wallpaper_red</DefaultWallpaper>
        <DefaultWallpaperLock>default_wallpaper_red</DefaultWallpaperLock>
        <DefaultWallpaperComponent>NA</DefaultWallpaperComponent>
    </PhoneColor>
</PhoneColors>
